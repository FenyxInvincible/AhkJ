{
    //keys with modifiers have to be above of keys without modifiers.
    //Modifier can be any valid key from keys.json
    "LSHIFT+1": {
        "onPress": {
            "macroClass": "SendSequence",
            "params": [
                {"key": "4", "action": "Press", "delay": 500},
                {"key": "3", "action": "Press", "delay": 500},
                {"key": "A", "action": "Press", "delay": 500}
            ]
        }
    },
    "1": {
        "onPress": {
            "macroClass": "RemapKey",
            "params": {
                "key": "2",
                "action": "Press"
            }
        },
        "onRelease": {
            "macroClass": "RemapKey",
            "params": {
                "key": "2",
                "action": "Release"
            }
        }
    },
    "2": {
        "onPress": {
            "macroClass": "SendSequence",
            "params": [
                {"key": "m", "delay": "100-1000"},
                {"key": "a", "delay": 100},
                {"key": "c", "delay": "100-1000"},
                {"key": "r"},
                {"key": "o", "delay": 100}
            ]
        }
    },
    "3": {
        "onPress": {
            "macroClass": "SpamSequence",
            "params": [
                {"key": "m", "delay": 100},
                {"key": "a", "delay": 100},
                {"key": "c", "delay": 100},
                {"key": "r", "delay": 100},
                {"key": "o", "delay": 100}
            ]
        }
    },
    "4": {
        "onPress": {
            "macroClass": "Gesture",
            "params": {
                "UP": "5",//link to macro below
                "DOWN": "6",
                "LEFT": "macro1", //link to virtual key macro1. case independent
                "RIGHT": "macro2" //link to virtual key macro2. case independent
            }
        }
    },
    "5": {
        "onPress": {
            "macroClass": "Crosshair",
            "params": {
                "imagePath": "profiles/crosshair.png",
                "scaleX": "60",
                "scaleY": "60"
            }
        }
    },
    "7": {
        "onPress": {
            "macroClass": "DoubleAction",
            "params": {
                "longPressMs": 300,
                //key 1 will be sent, due to macro propagation macro which is bound on key "1" will be executed
                "shortKey": "1",
                "longKey": "2",
                "propagateCall": true
            }
        }
    },
    "8": {
        "onPress": {
            "macroClass": "Countdown",
            "params": {
                "duration": 3000,
                "icon": {
                    "imagePath": "profiles/macroJ.png",
                    "x": 400,
                    "y": 400,
                    "width": 100,
                    "height": 100
                },
                "font": {
                    "name": "TimesRoman",
                    "style": "BOLD",
                    "size": 20,
                    "color": {
                        "r": 255,
                        "g": 255,
                        "b": 0
                    }
                }
            }
        }
    },
    "macro1": {
        "onPress": {
            "macroClass": "SendSequence",
            "params": [
                {"key": "m", "delay": 16},
                {"key": "a", "delay": 16},
                {"key": "c", "delay": 16},
                {"key": "r", "delay": 16},
                {"key": "o", "delay": 16},
                {"key": "1", "delay": 16}
            ]
        }
    },
    "macro2": {
        "onPress": {
            "macroClass": "SendSequence",
            "params": [
                {"key": "m", "delay": 16},
                {"key": "a", "delay": 16},
                {"key": "c", "delay": 16},
                {"key": "r", "delay": 16},
                {"key": "o", "delay": 16},
                {"key": "2", "delay": 16}
            ]
        }
    },
    //photoshop example. short press will select brush, long(more that 500 ms by default) - pen
    "CAPSLOCK": {
        "onPress": {
            "macroClass": "DoubleAction",
            "params": {
                "shortKey": "B",//brush
                "longKey": "P"//pen
            }
        }
    },
    //photoshop example. Press right mouse button, move mouse up/down/right/left, release rmb.
    //without movement rmb will be sent
    "RMB": {
        "onPress": {
            "macroClass": "Gesture",
            "params": {
                "UP": "M",//selection
                "DOWN": "L",//lasso
                "LEFT": "G", //fill
                "RIGHT": "E" //Erase
            }
        }
    },
    "F11": {
        "onPress": {
            "macroClass": "Debug"
        }
    }
}