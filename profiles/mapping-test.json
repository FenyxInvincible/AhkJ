{
    //keys with modifiers have to be above of keys without modifiers.
    //Modifier can be any valid key from keys.json
    "LSHIFT+1": {
        "onPress": {
            "macroClass": "local.autohotkey.data.macro.SendSequence",
            "params": [
                {"key": "4", "action": "Press", "delay": 500},
                {"key": "3", "action": "Press", "delay": 500},
                {"key": "A", "action": "Press", "delay": 500}
            ]
        }
    },
    "1": {
        "onPress": {
            "macroClass": "local.autohotkey.data.macro.RemapKey",
            "params": {
                "key": "2",
                "action": "Press"
            }
        },
        "onRelease": {
            "macroClass": "local.autohotkey.data.macro.RemapKey",
            "params": {
                "key": "2",
                "action": "Release"
            }
        }
    },
    "2": {
        "onPress": {
            "macroClass": "local.autohotkey.data.macro.SpamSequence",
            "params": [
                {"key": "2", "action": "Press", "delay": 500},
                {"key": "3", "action": "Press", "delay": 100},
                {"key": "A", "action": "Press", "delay": 1000}
            ]
        }
    },
    "3": {
        "onPress": {
            "macroClass": "local.autohotkey.data.macro.SpamSequence",
            "params": [
                {"key": "2", "action": "Press", "delay": 500},
                {"key": "3", "action": "Press", "delay": 100},
                {"key": "A", "action": "Press", "delay": 1000}
            ]
        }
    },
    "4": {
        "onPress": {
            "macroClass": "local.autohotkey.data.macro.Gesture",
            "params": {
                "UP": "5",//link to macro below
                "DOWN": "6",
                "LEFT": "7",
                "RIGHT": "8"
            }
        }
    },
    "5": {
        "onPress": {
            "macroClass": "local.autohotkey.data.macro.Crosshair",
            "params": {
                "imagePath": "profiles/crosshair.png",
                "scaleX": "60",
                "scaleY": "60"
            }
        }
    },
    "6": {
        "onPress": {
            "macroClass": "local.autohotkey.data.macro.DebugMacro",
            "params": []
        }
    },
    "7": {
        "onPress": {
            "macroClass": "local.autohotkey.data.macro.DoubleAction",
            "params": {
                "longPressMs": 300,
                //key 1 will be sent, due to macro propagation macro which is bound on key "1" will be executed
                "shortKey": "1",
                "longKey": "2",
                "propagateCall": true
            }
        }
    }
}